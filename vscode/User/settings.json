{
  "terminal.integrated.shell.windows": "C:\\Windows\\System32\\bash.exe",
  "workbench.startupEditor": "newUntitledFile",
  "editor.minimap.enabled": false,
  "projectManager.sortList": "Name",
  "workbench.colorTheme": "Material Theme Palenight",
  "workbench.iconTheme": "eq-material-theme-icons",
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "files.eol": "\n",
  "workbench.colorCustomizations": {
    "editorWarning.border": "#336699",
    "editorWarning.foreground": "#336699",
    "editorError.border": "#ff0800",
    "editorError.foreground": "#ff0800",
    "statusBar.background": "#8FBCBB",
    "statusBar.noFolderBackground": "#8FBCBB",
    "statusBar.debuggingBackground": "#8FBCBB",
    "statusBar.foreground": "#434C5E"
  },
  "npm-intellisense.importES6": true,
  "npm-intellisense.importQuotes": "'",
  "npm-intellisense.importLinebreak": ";\r\n",
  "npm-intellisense.importDeclarationType": "const",
  "npm-intellisense.scanDevDependencies": true,
  "npm-intellisense.showBuildInLibs": true,
  "editor.formatOnSave": true,
  "editor.fontFamily": "consolas, Droid Sans Mono Slashed for Powerline",
  "editor.lineHeight": 20,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/dist": true
  },
  "esQuotes.defaultQuote": "\"",
  "material-icon-theme.showUpdateMessage": false,
  "editor.snippetSuggestions": "top",
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    // "javascript": "javascriptreact",
    "vue-html": "html",
    "plaintext": "jade"
  },
  "telemetry.enableTelemetry": false,
  "cSpell.enabledLanguageIds": [
    "javascript",
    "shellscript",
    "json",
    "markdown"
  ],
  "cSpell.userWords": [
    "Promisified",
    "childrens",
    "dedupe",
    "deduped",
    "dups",
    "hasher",
    "maindir",
    "maindirs",
    "myutil",
    "promisify",
    "stringify",
    "todoc",
    "toscav",
    "xform"
  ],
  "editor.lineNumbers": "relative",
  "bracketPairColorizer.highlightActiveScope": true,
  "bracketPairColorizer.forceUniqueOpeningColor": false,
  "rewrap.wrappingColumn": 60,
  "vim.disableExtension": false,
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.handleKeys": {
    "<C-w>": false,
    "<C-b>": false
  },
  "vim.statusBarColorControl": false,
  "vim.statusBarColors": {
    "normal": ["#8FBCBB", "#434C5E"],
    "insert": "#BF616A",
    "visual": "#B48EAD",
    "visualline": "#B48EAD",
    "visualblock": "#A3BE8C",
    "replace": "#D08770"
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["g", "o"],
      "after": ["o", "<Esc>", "k"]
    },
    {
      "before": ["g", "O"],
      "after": ["O", "<Esc>"]
    },
    {
      "before": ["<leader>", "<leader>", "o"],
      "after": ["O", "<Esc>", "j", "o", "<Esc>", "k", "i"]
    },
    {
      "before": ["S"],
      "after": ["c", "i", "w", "<C-r>", "0", "<Esc>"]
    }
  ],
  "files.associations": {
    "*.json": "jsonc",
    "*.zshrc_*": "shellscript"
  }
}
